  HostName 107.23.78.53
  User ubuntu
  IdentityFile VPC-demo-key.pem
  PasswordAuthentication no
  ServerAliveInterval 60
  TCPKeepAlive yes
  ControlMaster auto
  ControlPath /tmp/ansible-%r@%h:%p
  ControlPersist 15m
  StrictHostKeyChecking no
  LogLevel QUIET

